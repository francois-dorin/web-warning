{
  "manifest_version": 3,
  "name": "__EXTENSION_NAME__",
  "version": "__EXTENSION_VERSION__",
  "description": "__EXTENSION_DESCRIPTION__",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "alarms"
  ],
  "background": {
    "scripts": [
      "service-worker.js"
    ]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "web-warning@custom-dev.fr",
      "strict_min_version": "109.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icone-app_ia16.png",
      "32": "icone-app_ia32.png",
      "48": "icone-app_ia48.png",
      "64": "icone-app_ia64.png",
      "128": "icone-app_ia128.png"
    }
  },
  "icons": {
    "16": "icone-app_ia16.png",
    "32": "icone-app_ia32.png",
    "48": "icone-app_ia48.png",
    "64": "icone-app_ia64.png",
    "128": "icone-app_ia128.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "web_accessible_resources": [ 
    {
      "resources": [ "options.html", "options.js", "popup.html", "popup.js" ],
      "extension_ids": []
    } 
  ]
}